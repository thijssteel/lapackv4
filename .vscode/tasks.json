{
   "version": "2.0.0",
   "tasks": [
      {
         "type": "cppbuild",
         "label": "Build active file using GNU make",
         "command": "/usr/bin/make",
         "args": [
            "bin/${fileBasenameNoExtension}"
         ],
         "options": {
            "cwd": "${workspaceFolder}"
         },
         "group": "build"
      }
   ]
}